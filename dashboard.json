                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "legend": {
                            "enabled": true
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [
                                    1863519
                                ],
                                "query": "SELECT latest(avg)\nFROM pingCheck \nFACET deviceName\nTIMESERIES MAX\nSINCE 1 hour ago"
