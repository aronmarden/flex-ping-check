                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [
                                    1863519
                                ],
                                "query": "SELECT sum(packetLoss) \nFROM pingCheck \nFACET deviceName, deviceAddr"
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        },
                        "thresholds": [
                            {
                                "columnName": "Total Packet Loss",
