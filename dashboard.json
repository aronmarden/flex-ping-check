                            {
                                "columnName": "Total Packet Loss",
                                "from": 1,
                                "severity": "critical",
                                "to": 9999999999
                            }
                        ]
                    }
                },
                {
                    "title": "Devices Ping Packloss Over Time",
                    "layout": {
                        "column": 6,
                        "row": 1,
                        "width": 7,
                        "height": 4
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "legend": {
                            "enabled": true
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [
                                    1863519
                                ],
                                "query": "SELECT latest(packetLoss)\nFROM pingCheck \nFACET deviceName\nTIMESERIES MAX"
                            }
                        ],
                        "nullValues": {
                            "nullValue": "default"
                        },
